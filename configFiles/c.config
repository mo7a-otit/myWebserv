server                            {
    server_name : 0.0.0.0;
    host :  0.0.0.0;
    client_max_body_size : 999999999999999999;

    listen : 8080;
    location /{
        root : ./;
        allow_methods : GET POST;
        autoindex : on;
        index : index.html;
    }

    location /cgi-bin{
        allow_methods : POST GET;
        autoindex : on;
        upload : on;
        cgi : on;
        index : index.php;
        root : ./cgi/;
        cgi_path : .py /usr/bin/python3 .sh /bin/bash .php /usr/bin/php-cgi;
    }
    error_page 404 : error_pages/404.html; 
}
